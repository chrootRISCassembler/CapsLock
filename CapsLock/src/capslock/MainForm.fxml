<HBox fx:id="RootHBox" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="capslock.MainFormController">
   <children>
      <TilePane fx:id="LeftTilePane" minHeight="0.0" minWidth="0.0" style="-fx-background-color: red;" HBox.hgrow="ALWAYS" />
      <VBox fx:id="RightVBox" prefHeight="200.0" prefWidth="100.0" style="-fx-background-color: blue;" HBox.hgrow="NEVER">
         <children>
            <StackPane fx:id="ViewStackPane" prefHeight="150.0" prefWidth="200.0" />
            <Label fx:id="NameLabel" text="Label" />
            <ScrollPane fx:id="LabelScroller" prefHeight="200.0" prefWidth="200.0">
               <content>
                  <Label fx:id="DescriptionLabel" text="Label" />
               </content>
            </ScrollPane>
         </children>
      </VBox>
   </children>
</HBox>
